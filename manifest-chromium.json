{
  "manifest_version": 3,
  "name": "LibrePhotos Full-Screen Viewer",
  "version": "1.0",
  "description": "Extension to view images in full-screen on LibrePhotos.",
  "permissions": ["activeTab", "contextMenus", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [],
  "action": {}
}
